<% if(success && success.length){ %>
<div
  class="w-1/2 mx-auto text-center p-4 mb-4 text-sm text-green-800 rounded-lg bg-green-100 dark:bg-gray-800 dark:text-green-400"
>
  <%= success %>
</div>
<% } %> <% if(error && error.length){ %>
<div
  class="w-1/2 mx-auto text-center p-4 mb-4 text-sm text-red-800 rounded-lg bg-red-50 dark:bg-gray-800 dark:text-red-400"
>
  <%= error %>
</div>
<% } %>
